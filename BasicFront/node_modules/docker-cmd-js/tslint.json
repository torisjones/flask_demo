{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "ordered-imports": [false],
      "variable-name": [false],
      "array-type": [false],
      "member-access": [false],
      "trailing-comma": [false],
      "no-string-literal": false,
      "arrow-parens": false,
      "no-console": [false],
      "prefer-for-of": false
    },
    "rulesDirectory": []
}
